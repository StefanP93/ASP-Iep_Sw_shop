<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - IEP sw Shop</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body >
   
    <div class="container body-content">
        @RenderBody()
        
    </div>
    <div style="position: fixed; bottom: 0; width:100%; background-color:#180073; color:#ffffff;">
        <hr />
        <footer>
            <center>
                <p> @DateTime.Now.Year <br>IEP Project Stefan Pantelic 511/12</p>
            </center>
        </footer>



    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

</html>
